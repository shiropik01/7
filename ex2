print("–ü—Ä–æ–≥—Ä–∞–º–º–∞ ¬´–û—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –∑–∞–∫–∞–∑–∞¬ª, –∫–æ—Ç–æ—Ä–∞—è –ø–æ –≤–≤–µ–¥–µ–Ω–Ω–æ–º—É –∫–æ–¥—É —Å—Ç–∞—Ç—É—Å–∞ –∑–∞–∫–∞–∑–∞ –≤—ã–≤–æ–¥–∏—Ç –¥–µ—Ç–∞–ª—å–Ω–æ–µ –æ–ø–∏—Å–∞–Ω–∏–µ —Ç–µ–∫—É—â–µ–≥–æ —Å—Ç–∞—Ç—É—Å: –∞pending (–≤ –æ–∂–∏–¥–∞–Ω–∏–∏), processing (–≤ –æ–±—Ä–∞–±–æ—Ç–∫–µ), shipped (–æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ), delivered (–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æ), cancelled (–æ—Ç–º–µ–Ω–µ–Ω–æ)")

status = input("–í–≤–µ–¥–∏—Ç–µ —Å—Ç–∞—Ç—É—Å –∑–∞–∫–∞–∑–∞: ")

print("=" * 30)
print("üì¶ –°–¢–ê–¢–£–° –í–ê–®–ï–ì–û –ó–ê–ö–ê–ó–ê üì¶")
print("=" * 30)

match status:
    case "pending":
        print("–°—Ç–∞—Ç—É—Å: –í –æ–∂–∏–¥–∞–Ω–∏–∏ ‚è≥")
        print("–û–ø–∏—Å–∞–Ω–∏–µ: –ó–∞–∫–∞–∑ –æ–∂–∏–¥–∞–µ—Ç –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –∏ –æ–±—Ä–∞–±–æ—Ç–∫–∏")
        print("–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è: 1-2 —á–∞—Å–∞")
        print("–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –û–∂–∏–¥–∞–π—Ç–µ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –∑–∞–∫–∞–∑–∞")
    case "processing":
        print("–°—Ç–∞—Ç—É—Å: –í –æ–±—Ä–∞–±–æ—Ç–∫–µ üîß")
        print("–û–ø–∏—Å–∞–Ω–∏–µ: –í–∞—à –∑–∞–∫–∞–∑ —Å–æ–±–∏—Ä–∞–µ—Ç—Å—è –∏ –ø–æ–¥–≥–æ—Ç–∞–≤–ª–∏–≤–∞–µ—Ç—Å—è –∫ –æ—Ç–ø—Ä–∞–≤–∫–µ")
        print("–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è: 1-3 –¥–Ω—è")
        print("–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –°–∫–æ—Ä–æ –≤–∞—à –∑–∞–∫–∞–∑ –±—É–¥–µ—Ç –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω")
    case "shipped":
        print("–°—Ç–∞—Ç—É—Å: –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ üöö")
        print("–û–ø–∏—Å–∞–Ω–∏–µ: –í–∞—à –∑–∞–∫–∞–∑ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ –ø—É—Ç–∏ –∫ –≤–∞–º")
        print("–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è: 2-5 –¥–Ω–µ–π")
        print("–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –°–ª–µ–¥–∏—Ç–µ –∑–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è–º–∏ –æ –¥–æ—Å—Ç–∞–≤–∫–µ")
    case "delivery":
        print("–°—Ç–∞—Ç—É—Å: –î–æ—Å—Ç–∞–≤–ª–µ–Ω–æ ‚úÖ")
        print("–û–ø–∏—Å–∞–Ω–∏–µ: –ó–∞–∫–∞–∑ —É—Å–ø–µ—à–Ω–æ –¥–æ—Å—Ç–∞–≤–ª–µ–Ω –ø–æ–ª—É—á–∞—Ç–µ–ª—é")
        print("–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è: –ó–∞–∫–∞–∑ –¥–æ—Å—Ç–∞–≤–ª–µ–Ω")
        print("–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –ü—Ä–æ–≤–µ—Ä—å—Ç–µ —Ü–µ–ª–æ—Å—Ç–Ω–æ—Å—Ç—å —Ç–æ–≤–∞—Ä–∞")
    case "cancelled":
        print("–°—Ç–∞—Ç—É—Å: –û—Ç–º–µ–Ω–µ–Ω–æ ‚ùå")
        print("–û–ø–∏—Å–∞–Ω–∏–µ: –ó–∞–∫–∞–∑ –±—ã–ª –æ—Ç–º–µ–Ω–µ–Ω")
        print("–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è: –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—Ç–º–µ–Ω—ã")
        print("–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –°–≤—è–∂–∏—Ç–µ—Å—å —Å –ø–æ–¥–¥–µ—Ä–∂–∫–æ–π –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –¥–µ—Ç–∞–ª–µ–π")
    case _:
        print(f"–û—à–∏–±–∫–∞: –ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Å—Ç–∞—Ç—É—Å")
        print("–î–æ—Å—Ç—É–ø–Ω—ã–µ —Å—Ç–∞—Ç—É—Å—ã: pending, processing, shipped, delivery, cancelled")
